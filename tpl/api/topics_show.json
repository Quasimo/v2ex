[
    {% for topic in topics %}
    {
        "id" : {{ topic.num }},
        "title" : "{{ topic.title|escapejs }}",
        "url" : "http://{{ site.domain }}/t/{{ topic.num }}",
        "content" : "{{ topic.content|escapejs }}",
        "content_rendered" : "{{ topic.content_rendered|escapejs}}",
        "replies" : {{ topic.replies }}
    },
    {% endfor %}
]